{
  "name": "tapi-client-js",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --fix .",
    "seed": "NODE_ENV=test knex seed:run && node seeds/redis/populate-lists.mjs",
    "seed:pop": "NODE_ENV=populate knex seed:run",
    "test": "NODE_ENV=test jest --runInBand",
    "test:qa": "source .env-tapi-qa && NODE_ENV=test jest --runInBand",
    "test:sandbox": "source .env-tapi-sandbox && NODE_ENV=test jest --runInBand",
    "test:all": "npm run test:qa && npm run test:sandbox",
    "tapi": "node ./tapi/tools/execute.mjs",
    "upload-test-report": "aws s3 cp --recursive ./html-report/ s3://tapitestreportstack-tapitestreportbucket4eea3b17-c8jwb9b43yqk/",
    "invalidate-test-report": "aws cloudfront create-invalidation --distribution-id E1DQQHLZFTPAHA --paths '/*'",
    "publish-test-report": "npm run upload-test-report && npm run invalidate-test-report"
  },
  "dependencies": {
    "@stdlib/datasets-us-states-abbr": "^0.0.8",
    "@stdlib/datasets-us-states-names": "^0.0.8",
    "axios": "^1.2.3",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.4",
    "ejs": "^3.1.8",
    "express": "~4.18.1",
    "form-data": "^4.0.0",
    "http-errors": "~2.0.0",
    "jest-html-reporters": "^3.1.4",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.0",
    "node-redis": "^0.1.7",
    "pg": "^8.8.0",
    "pug": "^3.0.2",
    "redis": "^4.6.13",
    "redis-cli": "^2.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.1.2",
    "k6": "^0.0.0",
    "leaked-handles": "^5.2.0",
    "node-random-name": "^1.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "random-words": "^1.2.0",
    "rdme": "^8.6.6",
    "supertest": "^6.2.4",
    "swagger-autogen": "^2.23.6",
    "swagger-ui-express": "^5.0.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "pageTitle": "Transact API Test Report",
          "filename": "index.html",
          "darkTheme": true
        }
      ]
    ],
    "testPathIgnorePatterns": [
      "/cdk/"
    ],
    "setupFilesAfterEnv": [
      "./setupParty.js"
    ]
  }
}
